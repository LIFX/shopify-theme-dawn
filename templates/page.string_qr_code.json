    "332a1804-754d-475c-8f99-8c2c0e15083c": {
      "type": "image-with-text",
      "blocks": {
        "template--15996389228725__332a1804-754d-475c-8f99-8c2c0e15083c-heading-1": {
          "type": "heading",
          "settings": {
            "heading": "Connect to the LIFX App"
          }
        },
        "template--15996389228725__332a1804-754d-475c-8f99-8c2c0e15083c-text-1": {
          "type": "text",
          "settings": {
            "text": "<p>Download the LIFX app for <a href=\"https:\/\/apps.apple.com\/us\/app\/lifx\/id657758311\" target=\"_blank\" title=\"LIFX iOS App on Apple App Store\">iOS<\/a> or <a href=\"https:\/\/play.google.com\/store\/apps\/details?id=com.lifx.lifx&hl=en_AU&gl=US\" target=\"_blank\" title=\"LIFX Android App on Google Play Store\">Android<\/a> to get started. <\/p><ul><li>Open the app<\/li><li>Hit the '+' in the bottom right to add a new product.<\/li><li>Follow the prompts and the app will guide you through the setup process.<\/li><\/ul><p><strong>Having Trouble Connecting? <\/strong>Make sure you're connecting to your 2.4gHz Wi-Fi network. <\/p><p><strong>Still having trouble? <\/strong>You can start a chat with us <a href=\"https:\/\/support.lifx.com\/hc\/en-us\" title=\"LIFX Support\">here<\/a> at any time.<\/p>"
          }
        },
