    "custom_liquid_7TYgpb": {
      "type": "custom-liquid",
      "custom_css": [

      ],
      "settings": {
        "custom_liquid": "<style>\n input[type=\"range\"] {\n  \/* removing default appearance *\/\n  -webkit-appearance: none;\n  appearance: none; \n  \/* creating a custom design *\/\n  width: 100%;\n  cursor: pointer;\n  outline: none;\n  margin:2rem 0 2rem 0;\n  border-radius: 15px;\n  \/*  overflow: hidden;  remove this line*\/\n  \n  \/* New additions *\/\n  height: 15px;\n  background: #eee9e9;\n}\n\n\/* Thumb: webkit *\/\ninput[type=\"range\"]::-webkit-slider-thumb {\n  \/* removing default appearance *\/\n  -webkit-appearance: none;\n  appearance: none; \n  \/* creating a custom design *\/\n  height: 30px;\n  width: 30px;\n  background-color: #E2FC52;\n  border-radius: 50%;\n  border: none;\n\n  \/* box-shadow: -407px 0 0 400px #f50; emove this line *\/\n  transition: .2s ease-in-out;\n}\n\n\/* Thumb: Firefox *\/\ninput[type=\"range\"]::-moz-range-thumb {\n  height: 15px;\n  width: 15px;\n  background-color: #E2FC52;\n  border-radius: 50%;\n  border: none;\n  \n  \/* box-shadow: -407px 0 0 400px #f50; emove this line *\/\n  transition: .2s ease-in-out;\n}\n\n\/* Hover, active & focus Thumb: Webkit *\/\n\ninput[type=\"range\"]::-webkit-slider-thumb:hover {\n  box-shadow: 0 0 0 10px rgba(226,252,82, .2)\n}\ninput[type=\"range\"]:active::-webkit-slider-thumb {\n  box-shadow: 0 0 0 13px rgba(226,252,82, .2)\n}\ninput[type=\"range\"]:focus::-webkit-slider-thumb {\n  box-shadow: 0 0 0 13px rgba(226,252,82, .2)\n}\n\n\/* Hover, active & focus Thumb: Firfox *\/\n\ninput[type=\"range\"]::-moz-range-thumb:hover {\n  box-shadow: 0 0 0 10px rgba(226,252,82, .1)\n}\ninput[type=\"range\"]:active::-moz-range-thumb {\n  box-shadow: 0 0 0 13px rgba(226,252,82, .2)\n}\ninput[type=\"range\"]:focus::-moz-range-thumb {\n  box-shadow: 0 0 0 13px rgba(226,252,82, .2)    \n}\n\n\/*=============\nAesthetics \n=========================*\/\n\nbody {\n  font-family: system-ui;\n}\n\nh1 {\n  color: #4b4949; \n  text-align: left;\n}\n\n.wrapper {\n  color: #4b4949; \n  background: #E2FC52;\n  max-width: 400px;\n  width: 100%;\n  height: 300px;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  margin: 0 auto;\n}\n\n.range {\n  display: flex;\n  align-items: center;\n  gap: 1rem;\n  max-width: 500px;\n  margin: 0 auto;\n  height: 4rem;\n  width: 80%;\n  background: #fff;\n  padding: 0px 10px;\n}\n\n.value {\n  font-size: 26px;    \n  width: 50px;\n  text-align: center;\n}\n\n\nselect {\n\n\/* styling *\/\nbackground-color: white;\nborder: thin solid #E2FC52;\nborder-radius: 4px;\ndisplay: inline-block;\nfont: inherit;\nline-height: 1.5em;\npadding: 0.5em 3.5em 0.5em 1em;\n\n\/* reset *\/\n\nmargin: 0;      \n-webkit-box-sizing: border-box;\n-moz-box-sizing: border-box;\nbox-sizing: border-box;\n-webkit-appearance: none;\n-moz-appearance: none;\n}\n\n\n\/* arrows *\/\n\nselect.classic {\nbackground-image:\n  linear-gradient(45deg, transparent 50%, #000000 50%),\n  linear-gradient(135deg, #000000 50%, transparent 50%),\n  linear-gradient(to right, #E2FC52, #E2FC52);\nbackground-position:\n  calc(100% - 20px) calc(1em + 2px),\n  calc(100% - 15px) calc(1em + 2px),\n  100% 0;\nbackground-size:\n  5px 5px,\n  5px 5px,\n  2.5em 2.5em;\nbackground-repeat: no-repeat;\n}\n\nselect.classic:focus {\nbackground-image:\n  linear-gradient(45deg, white 50%, transparent 50%),\n  linear-gradient(135deg, transparent 50%, white 50%),\n  linear-gradient(to right, gray, gray);\nbackground-position:\n  calc(100% - 15px) 1em,\n  calc(100% - 20px) 1em,\n  100% 0;\nbackground-size:\n  5px 5px,\n  5px 5px,\n  2.5em 2.5em;\nbackground-repeat: no-repeat;\nborder-color: grey;\noutline: 0;\n}\n\n\n\n\nselect.round {\nbackground-image:\n  linear-gradient(45deg, transparent 50%, gray 50%),\n  linear-gradient(135deg, gray 50%, transparent 50%),\n  radial-gradient(#ddd 70%, transparent 72%);\nbackground-position:\n  calc(100% - 20px) calc(1em + 2px),\n  calc(100% - 15px) calc(1em + 2px),\n  calc(100% - .5em) .5em;\nbackground-size:\n  5px 5px,\n  5px 5px,\n  1.5em 1.5em;\nbackground-repeat: no-repeat;\n}\n\nselect.round:focus {\nbackground-image:\n  linear-gradient(45deg, white 50%, transparent 50%),\n  linear-gradient(135deg, transparent 50%, white 50%),\n  radial-gradient(gray 70%, transparent 72%);\nbackground-position:\n  calc(100% - 15px) 1em,\n  calc(100% - 20px) 1em,\n  calc(100% - .5em) .5em;\nbackground-size:\n  5px 5px,\n  5px 5px,\n  1.5em 1.5em;\nbackground-repeat: no-repeat;\nborder-color: green;\noutline: 0;\n}\n\n\n\n\n\nselect.minimal {\nbackground-image:\n  linear-gradient(45deg, transparent 50%, gray 50%),\n  linear-gradient(135deg, gray 50%, transparent 50%),\n  linear-gradient(to right, #ccc, #ccc);\nbackground-position:\n  calc(100% - 20px) calc(1em + 2px),\n  calc(100% - 15px) calc(1em + 2px),\n  calc(100% - 2.5em) 0.5em;\nbackground-size:\n  5px 5px,\n  5px 5px,\n  1px 1.5em;\nbackground-repeat: no-repeat;\n}\n\nselect.minimal:focus {\nbackground-image:\n  linear-gradient(45deg, green 50%, transparent 50%),\n  linear-gradient(135deg, transparent 50%, green 50%),\n  linear-gradient(to right, #ccc, #ccc);\nbackground-position:\n  calc(100% - 15px) 1em,\n  calc(100% - 20px) 1em,\n  calc(100% - 2.5em) 0.5em;\nbackground-size:\n  5px 5px,\n  5px 5px,\n  1px 1.5em;\nbackground-repeat: no-repeat;\nborder-color: green;\noutline: 0;\n}\n\n\nselect:-moz-focusring {\ncolor: transparent;\ntext-shadow: 0 0 0 #000;\n}\n<\/style>\n\n\n<div class=\"image-with-text page-width color-scheme-background-1\" >\n  <div class=\"image-with-text__grid color-background-1 grid grid--gapless grid--1-col grid--2-col-tablet gradient image-with-text__grid--reverse\">\n    <div class=\"grid__item\">\n      <div class=\"image-with-text__media image-with-text__media--adapt media\" style=\"padding-bottom: 100.0%;\" ><img srcset=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0219\/0638\/files\/07_LIFX_Path_Square_Gallery.jpg?v=1707097070 165w,https:\/\/cdn.shopify.com\/s\/files\/1\/0219\/0638\/files\/07_LIFX_Path_Square_Gallery.jpg?v=1707097070 360w,https:\/\/cdn.shopify.com\/s\/files\/1\/0219\/0638\/files\/07_LIFX_Path_Square_Gallery.jpg?v=1707097070 535w,https:\/\/cdn.shopify.com\/s\/files\/1\/0219\/0638\/files\/07_LIFX_Path_Square_Gallery.jpg?v=1707097070 750w,https:\/\/cdn.shopify.com\/s\/files\/1\/0219\/0638\/files\/07_LIFX_Path_Square_Gallery.jpg?v=1707097070 1070w,\" src=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0219\/0638\/files\/07_LIFX_Path_Square_Gallery.jpg?v=1707097070\" sizes=\"(min-width: 1600px) 750px, (min-width: 750px) calc((100vw - 130px) \/ 2), calc((100vw - 50px) \/ 2)\" alt=\"\" loading=\"lazy\" width=\"1328\" height=\"1328\"><\/div>\n    <\/div>\n    <div class=\"grid__item\">\n      <div class=\"image-with-text__content image-with-text__content--adapt\"  style=\"padding:2.5rem\">\n        \n<!-- Lighting Type Sliders -->\n<h1>Distance Calculator<\/h1>\n<p id=\"lightsAdvice\" style=\"font-style: italic;\">1. Select the number of LIFX outdoor lights you wish to install.<\/p>\n<br>\n<table style=\"width:100%;box-shadow:none\">\n    <tr>\n      <th rowspan=\"2\" width=\"128px\" style=\"border:0\"> <img src=\"https:\/\/iqi5ztrp4574z016-2190638.shopifypreview.com\/cdn\/shop\/files\/01_LIFX_Path_Round_Gallery_1100x.jpg?v=1706828199\" width=\"100px\"><\/th>\n      <td style=\"border:0\"><span>LIFX Path Lights: <span id=\"pathLightsValue\">0<\/span><\/td>\n    <\/tr>\n    <tr >\n      <td style=\"border:0\"><input type=\"range\" id=\"pathLightsSlider\" min=\"0\" max=\"10\" value=\"0\"><\/td>\n    <\/tr>\n\n    <tr>\n        <th rowspan=\"2\" width=\"100px\" style=\"border:0\"> <img src=\"https:\/\/iqi5ztrp4574z016-2190638.shopifypreview.com\/cdn\/shop\/files\/04_LIFX_Spot_Round_Gallery_750x.jpg?v=1707097209\" width=\"100px\"><\/th>\n        <td style=\"border:0\"><span>LIFX Spot Lights: <span id=\"spotLightsValue\">0<\/span><\/td>\n      <\/tr>\n      <tr >\n        <td style=\"border:0\"><input type=\"range\" id=\"spotLightsSlider\" min=\"0\" max=\"10\" value=\"0\"><\/td>\n      <\/tr>\n\n      <tr>\n        <th rowspan=\"2\" width=\"128px\" style=\"border:0\"> <img src=\"https:\/\/iqi5ztrp4574z016-2190638.shopifypreview.com\/cdn\/shop\/files\/LIFX-Flex_16_Gallery_Outdoor_01_1100x.jpg?v=1706828337\" width=\"100px\"><\/th>\n        <td style=\"border:0\"><span>LIFX Neon Flex Outdoor: <span id=\"neonFlexValue\">0<\/span><\/td>\n      <\/tr>\n      <tr>\n        <td style=\"border:0\"><input type=\"range\" id=\"neonFlexSlider\" min=\"0\" max=\"10\" value=\"0\"><\/td>\n      <\/tr>\n\n\n\n  <\/table>\n\n\n\n\n\n\n\n\n\n<!-- Voltage Selector for Maximum Distance -->\n\n<div style=\"margin-top:3rem\">\n    \n    <p id=\"distanceAdvice\" style=\"font-style: italic;\">2. Select the voltage output terminal your transform has for an accurate calulation.<\/p>\n    <br>\n    <label for=\"voltageSelector\" style=\"font-weight: bolder;\">Choose Voltage: <\/label>\n\n    <select id=\"voltageSelector\" class=\"classic\">\n        <option value=\"15V\">15V<\/option>\n        <option value=\"12V\">12V<\/option>\n    <\/select>\n\n\n<\/div>\n\n\n\n<!-- Wire Gauge Maximum Distance -->\n<div style=\"margin-top:3rem\">\n    <p id=\"distanceAdvice\" style=\"font-style: italic;\">3. Based on your selections, you can install your lights up to a maximum distance from your transformer.<\/p>\n    <br>\n    <p><span style=\"font-weight: bolder;\">12 Gauge Wire: <\/span><span id=\"distance12AWG\">...<\/span><\/p>\n    <p><span style=\"font-weight: bolder;\">14 Gauge Wire: <\/span><span id=\"distance14AWG\">...<\/span><\/p>\n    <p><span style=\"font-weight: bolder;\">16 Gauge Wire: <\/span><span id=\"distance16AWG\">...<\/span><\/p>\n<\/div>\n\n<!-- Total Current and Term Voltage Display -->\n<div style=\"display:none\">\n    <h2>Electrical Calculation<\/h2>\n    <p id=\"totalCurrentDisplay\">Total Current: Calculating...<\/p>\n    <p id=\"termVoltageDisplay\">Terminal Voltage: Calculating...<\/p>\n<\/div>\n\n\n\n\n<script>\n    \/\/ Constants for ohms per meter for each wire gauge\n    const ohmsPerMeter = {\n        \"16AWG\": 0.0135,\n        \"14AWG\": 0.008,\n        \"12AWG\": 0.0053\n    };\n    const minimumConverterVoltage = 10.8;\n\n    \/\/ DOM elements\n    var pathLightsSlider = document.getElementById(\"pathLightsSlider\");\n    var spotLightsSlider = document.getElementById(\"spotLightsSlider\");\n    var neonFlexSlider = document.getElementById(\"neonFlexSlider\");\n    var voltageSelector = document.getElementById(\"voltageSelector\");\n\n    function calculateAndUpdateEverything() {\n        var pathLightsCount = parseInt(pathLightsSlider.value, 10);\n        var spotLightsCount = parseInt(spotLightsSlider.value, 10);\n        var neonLightsCount = parseInt(neonFlexSlider.value, 10);\n        var pathCurrent, spotCurrent, neonCurrent, termVoltage, totalCurrent;\n        if (voltageSelector.value === \"12V\") {\n            pathCurrent = 3.8;\n            spotCurrent = 3.3;\n            neonCurrent = 6.7;\n            termVoltage = -0.0257 * (pathLightsCount * pathCurrent + spotLightsCount * spotCurrent + neonLightsCount * neonCurrent) + 12.3;\n            totalCurrent = (pathLightsCount * pathCurrent) + (spotLightsCount * spotCurrent) + (neonLightsCount * neonCurrent);\n        } else if (voltageSelector.value === \"15V\") {\n            pathCurrent = 3.2;\n            spotCurrent = 3.2;\n            neonCurrent = 5.9;\n            termVoltage = -0.0307 * (pathLightsCount * pathCurrent + spotLightsCount * spotCurrent + neonLightsCount * neonCurrent) + 14.264;\n            totalCurrent = (pathLightsCount * pathCurrent) + (spotLightsCount * spotCurrent) + (neonLightsCount * neonCurrent);\n        } else {\n            alert(\"Please select a valid voltage option.\");\n            return;\n        }\n   \n        document.getElementById(\"totalCurrentDisplay\").textContent = \"Total Current: \" + totalCurrent.toFixed(2) + \" A\";\n        document.getElementById(\"termVoltageDisplay\").textContent = \"Terminal Voltage: \" + termVoltage.toFixed(2) + \" V\";\n\n\/\/ Update distances for each wire gauge\n        [\"12AWG\", \"14AWG\", \"16AWG\"].forEach(gauge => {\n            var distance = (((((termVoltage - minimumConverterVoltage) \/ totalCurrent) \/ 2) \/ ohmsPerMeter[gauge])* 3.28084);\n            if (isFinite(distance)) {\n              document.getElementById(`distance${gauge}`).textContent = `${distance.toFixed(0)} ft`;\n            }\n            else {\n              document.getElementById(`distance${gauge}`).textContent = '...';\n            }\n        });\n\n        document.getElementById(\"pathLightsValue\").textContent = pathLightsCount.toString();\n        document.getElementById(\"spotLightsValue\").textContent = spotLightsCount.toString();\n        document.getElementById(\"neonFlexValue\").textContent = neonLightsCount.toString();\n    }\n\n    \/\/ Attach event listeners to sliders and voltage selector\n    [pathLightsSlider, spotLightsSlider, neonFlexSlider, voltageSelector].forEach(element => {\n        element.addEventListener('input', calculateAndUpdateEverything);\n    });\n\n    \/\/ Initial calculation and UI update\n    calculateAndUpdateEverything();\n<\/script>\n    <\/div> \n        \n      <\/div>\n    <\/div>\n  <\/div>\n<\/div>"
      }
    },
    "custom_liquid_cnK7QW": {
      "type": "custom-liquid",
      "settings": {
        "custom_liquid": "<div class=\"image-with-text page-width color-scheme-background-1\">\n  <div class=\"image-with-text__grid color-background-1 grid grid--gapless grid--1-col grid--2-col-tablet gradient image-with-text__grid--reverse\">\n    \n    <div class=\"grid__item\">\n      <div class=\"image-with-text__content image-with-text__content--adapt\" style=\"padding:2.5rem\">\n      <!-- Minimum Transformer Requirements -->\n<div style=\"margin-top:3rem;margin-right:3rem\">\n    <h1 style=\"text-align:left;width:100%\">Minimum Transformer Requirements<\/h1>\n    <p id=\"transformerAdvice\">We recommend the use of a 300W Transformer with a 15V Output Terminal. All calculations are based on 300W Transfomer.<\/p> \n    <p id=\"transformerAdvice\">As per manufacturer instructions, it is recommended not to exceed 80% of the advertised wattage. Recommended Transformers at The Home Depot:<\/p>\n    <img src=\"https:\/\/images.thdstatic.com\/productImages\/8eea3189-3d1f-41aa-907e-1a4a2cf265cd\/svn\/hampton-bay-outdoor-lighting-accessories-diy-300ps-e1_145.jpg\" width=\"100px\" style=\"border:1px #eee; border-style:solid; padding:5px; margin-right:20px; float:left\">\n    <p style=\"vertical-align:top\"><a href=\"https:\/\/www.homedepot.com\/p\/Hampton-Bay-Low-Voltage-300-Watt-Landscape-Lightin-Transformer-with-Dusk-to-Dawn-Sensor-DIY-300PS\/321117383\">Hampton Bay 300W Transformer<\/a><\/p>\n    <p style=\"vertical-align:top\"><a href=\"https:\/\/www.homedepot.com\/p\/Hampton-Bay-Low-Voltage-600-Watt-Landscape-Transformer-DIY-600PS\/206286484\">Hampton Bay 600W Transformer<\/a><\/p>\n<\/div>\n    \n    \n     <\/div> \n        \n      <\/div>\n\n      <div class=\"grid__item\">\n      <div class=\"image-with-text__media image-with-text__media--adapt media\" style=\"padding-bottom: 100.0%;\"><img srcset=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0219\/0638\/files\/07_LIFX_Path_Square_Gallery.jpg?v=1707097070 165w,https:\/\/cdn.shopify.com\/s\/files\/1\/0219\/0638\/files\/07_LIFX_Path_Square_Gallery.jpg?v=1707097070 360w,https:\/\/cdn.shopify.com\/s\/files\/1\/0219\/0638\/files\/07_LIFX_Path_Square_Gallery.jpg?v=1707097070 535w,https:\/\/cdn.shopify.com\/s\/files\/1\/0219\/0638\/files\/07_LIFX_Path_Square_Gallery.jpg?v=1707097070 750w,https:\/\/cdn.shopify.com\/s\/files\/1\/0219\/0638\/files\/07_LIFX_Path_Square_Gallery.jpg?v=1707097070 1070w,\" src=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0219\/0638\/files\/07_LIFX_Path_Square_Gallery.jpg?v=1707097070\" sizes=\"(min-width: 1600px) 750px, (min-width: 750px) calc((100vw - 130px) \/ 2), calc((100vw - 50px) \/ 2)\" alt=\"\" loading=\"lazy\" width=\"1328\" height=\"1328\"><\/div>\n    <\/div>\n    <\/div>\n  <\/div>\n<\/div>"
      }
    }
