    "84cdb3e9-0567-418f-b49e-fd221dbd318d": {
      "type": "image-with-text",
      "blocks": {
        "template--15996389228725__84cdb3e9-0567-418f-b49e-fd221dbd318d-heading-1": {
          "type": "heading",
