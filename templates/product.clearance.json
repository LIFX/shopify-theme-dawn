        "f1da6ca5-9156-4723-9a4d-fdfca828c5a6": {
          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "{%assign amazon_affiliate = product.metafields.my_fields.amazon_affiliate %} {%assign amazon_affiliate_fr = product.metafields.my_fields.fr_amazon_affiliate %} {%assign amazon_affiliate_de = product.metafields.my_fields.de_amazon_affiliate %}  <div id=\"AMZ_UK\" style=\"display:none\">\n{% if collection.handle != 'shop-the-app' %}\n  {% if amazon_affiliate %}\n  <label class=\"form__label\" style=\"margin-bottom:0px\">Available at:<\/label>\n  <label>\n    <a href=\"{{ amazon_affiliate }}\" target=\"_blank\">\n      <img src=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0219\/0638\/t\/21\/assets\/favicon_amazon.ico?v=1629853280\" width=\"16px\" height=\"16px\" style=\"vertical-align:middle;margin-right:5px\"><span class=\"form__label\" style=\"display:inherit\">Amazon<\/span><\/span><\/a>\n<span class=\"form__label\" style=\"display:inherit\">\n     {{ product.price | money_with_currency }}\n  <\/label>\n  {% endif %}\n{% endif %}\n<\/div>\n\n<div id=\"AMZ_DE\" style=\"display:none\"> {% if collection.handle != 'shop-the-app' %}\n  {% if amazon_affiliate_de %}\n  <label class=\"form__label\" style=\"margin-bottom:0px\">Available at:<\/label>\n  <label>\n    <a href=\"{{ amazon_affiliate_de }}\" target=\"_blank\">\n      <img src=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0219\/0638\/t\/21\/assets\/favicon_amazon.ico?v=1629853280\" width=\"16px\" height=\"16px\" style=\"vertical-align:middle;margin-right:5px\"><span class=\"form__label\" style=\"display:inherit\">Amazon<\/span><\/span><\/a>\n<span class=\"form__label\" style=\"display:inherit\">\n     {{ product.price | money_with_currency }}\n  <\/label>\n  {% endif %}\n{% endif %} <\/div>  \n<div id=\"AMZ_FR” style=\"display:none\"> {% if collection.handle != 'shop-the-app' %}\n  {% if amazon_affiliate_fr %}\n  <label class=\"form__label\" style=\"margin-bottom:0px\">Available at:<\/label>\n  <label>\n    <a href=\"{{ amazon_affiliate_fr }}\" target=\"_blank\">\n      <img src=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0219\/0638\/t\/21\/assets\/favicon_amazon.ico?v=1629853280\" width=\"16px\" height=\"16px\" style=\"vertical-align:middle;margin-right:5px\"><span class=\"form__label\" style=\"display:inherit\">Amazon<\/span><\/span><\/a>\n<span class=\"form__label\" style=\"display:inherit\">\n     {{ product.price | money_with_currency }}\n  <\/label>\n  {% endif %}\n\n{% endif %}  <\/div>\n     \n<script type=\"text\/javascript\">\nif(location.hostname == \"lifxshop.co.uk\") {\ndocument.getElementById(\"AMZ_UK\").style.display = \"\";\n} if(location.hostname == \"lifxshop.de”) {\ndocument.getElementById(\"AMZ_DE”).style.display = \"\";\n} if(location.hostname == \"lifxshop.fr”) {\ndocument.getElementById(\"AMZ_FR”).style.display = \"\";\n}\n<\/script>"
          }
